{
  "builds": [
    {
      "src": "storybook-static/**",
      "use": "@vercel/static"
    },
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/storybook/(.*)",
      "dest": "/storybook-static/$1"
    }
  ]
}
